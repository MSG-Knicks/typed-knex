{
  "name": "@wwwouter/typed-knex",
  "version": "1.5.0",
  "description": "Makes knex better by working with TypeScript",
  "dependencies": {
    "@types/flat": "0.0.28",
    "flat": "4.1.0",
    "mocha": "5.2.0",
    "reflect-metadata": "0.1.12"
  },
  "main": "./dist/src/index.js",
  "typings": "dist/src/index",
  "scripts": {
